   "source": [
    "import random\n",
    "import string\n",
    "\n",
    "length = int(input(\"Enter password length: \"))\n",
    "chars = string.ascii_letters + string.digits + string.punctuation\n",
    "password = \"\".join(random.choice(chars) for _ in range(length))\n",
    "\n",
    "print(\"Generated Password:\", password)"
   ]
